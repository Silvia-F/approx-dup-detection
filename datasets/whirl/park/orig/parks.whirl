new rel npspark(href:doc name:doc) 
new rel icepark(href:doc name:doc) 

consult "parks.stir"

new view testjoin
from icepark as p
and npspark as q
where p.name sim q.name
select p.name as pkey1
and q.name as qkey1
select p.href as pkey2
and q.href as qkey2
end

new view oracle
from icepark as p
and npspark as q
where p.href equal q.href
select p.name as pkey1
and q.name as qkey1
select p.href as pkey2
and q.href as qkey2
end
