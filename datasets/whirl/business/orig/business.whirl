new rel hooverweb(company:doc industry:doc web:str)
new rel iontech(company:doc web:str ticker:str industry:str)

consult "business.stir"

new view testjoin
from hooverweb as r
and iontech as s
where r.company sim s.company
select r.company as rkey1
and s.company as skey1
and r.web as rkey2
and s.web as skey2
end
 
new view oracle
from hooverweb as r
and iontech as s
where r.web equal s.web
select r.company as rkey1
and s.company as skey1
and r.web as rkey2
and s.web as skey2
end
