new rel f(name:doc key:str)
new rel z(name:doc key:str)

consult "restaurant.stir"

new view testjoin
from f
and z
where f.name sim z.name
select f.name as fname
and z.name as zname
and f.key as fkey
and z.key as zkey
end

new view oracle
from f
and z
where f.key sim z.key
select f.name as fname
and z.name as zname
and f.key as fkey
and z.key as zkey
end


